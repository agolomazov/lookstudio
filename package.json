{
  "name": "loopstudios-app",
  "version": "1.0.0",
  "description": "Тестовое приложение по верстке с tailwindcss",
  "main": "index.js",
  "scripts": {
    "styles": "npx tailwindcss -i ./src/styles/main.css -o ./src/styles/production.css --watch",
    "serve": "parcel ./src/index.html --port 1234 --open",
    "start": "concurrently \"npm run styles\" \"npm run serve\""
  },
  "keywords": [],
  "author": "Anton Golomazov",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "parcel": "^2.8.2",
    "tailwindcss": "^3.2.4"
  }
}
